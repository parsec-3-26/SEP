    <script type="text/javascript" language="javascript">



       

        // window.onunload = function () {
        //}

      var isOpen = "true";
      var COOKIE_NAME = "STATO";


     





      function addEvent(obj, evType, fn) {
          if (obj.addEventListener) {
              obj.addEventListener(evType, fn, false);
              return true;
          } else if (obj.attachEvent) {
              var r = obj.attachEvent("on" + evType, fn);
              return r;
          } else {
              return false;
          }
      }


      function SetDirty(el) {
          document.getElementById('StatoPaginaHidden').value = 'C';
      }


      function ResetDirty(el) {
          document.getElementById('StatoPaginaHidden').value = '';
      }




      //**********************************************************
      //QUESTA FUNZIONE DEVE ESSERE CHIAMATA IN PAGELOAD DELLA PAGINA CONTENUTO
      //**********************************************************
      function OnChangeHandler() {
          if (!document.getElementsByTagName) return;
          var inputs, selects, textareas, i;

          inputs = document.getElementsByTagName('input');
          for (i = 0; i < inputs.length; i++) {

              var dummy = (/dummy/).test(inputs[i].getAttribute("dummy"));
              var submit = (/submit/).test(inputs[i].getAttribute("type"));
              var hidden = (/hidden/).test(inputs[i].getAttribute("type"));

              var pagerInput = (/rcbInput/).test(inputs[i].className);

              if ((dummy == false) && (submit == false) && (hidden == false) && (pagerInput == false))
              {
                 inputs[i].onchange = function () { SetDirty(this); };
              }
          }

          textareas = document.getElementsByTagName('textarea');
          for (i = 0; i < textareas.length; i++) {
              textareas[i].onchange = function () { SetDirty(this) };
          }


          selects = document.getElementsByTagName('select');
          for (i = 0; i < selects.length; i++) {
              var dummySelect = (/dummy/).test(selects[i].getAttribute("dummy"));
              if (dummySelect == false) { 
                 selects[i].onchange = function () { SetDirty(this) };
              }
            
          }
      }

      //addEvent(window, 'load', OnChangeHandler);





      function HideConfirm() {
          var c = document.getElementById("confirm");
          c.style.visibility = "hidden";
      }


      function showHideConfirm() {
          var e = document.getElementById("confirmOverlay");
          var c = document.getElementById("confirm");
          if (e.style.visibility != "hidden") {
              e.style.visibility = "hidden";
              c.style.visibility = "hidden";
          }
          else {
              c.style.visibility = "visible";
              e.style.visibility = "visible";
              e.style.position = 'absolute';
              e.style.left = '0px';
              e.style.top = '0px';
              e.style.width = '100%';
              e.style.height = '100%';
              e.style.zIndex = 10000;
              e.style.backgroundColor = '#09718F';
              e.style.filter = "alpha(opacity=20)";
              e.style.opacity = "0.2";
          }
      }

      //**********************************************************
      // GESTIONE MENU NAVIGAZIONE
      //**********************************************************
      function OnClientItemClicking(sender, eventArgs) {
          var item = eventArgs.get_item();
          var stato = document.getElementById('StatoPaginaHidden').value;
          if (stato == 'C') {
              showHideConfirm();
              eleClicked = item.get_linkElement();
              eventArgs.set_cancel(true)
              blockUI = true;
              commandType = 'MenuItem';
          }
      }

      var eleClicked = '';
      var blockUI = true;
      var masterTable = null;
      var commandName = '';
      var commandArgument = '';
      var commandType = 'Button';

      //**********************************************************
      // GESTIONE TOOLBAR PAGINA
      //**********************************************************
      function OnClientButtonClicking(sender, eventArgs) {
          var button = eventArgs.get_item();
          var commandName = button.get_commandName();
          if ((commandName == "Nuovo") || (commandName == "Annulla") || (commandName == "CreteGU5")) {
              var stato = document.getElementById('StatoPaginaHidden').value;
              if (stato == 'C') {
                  showHideConfirm();
                  eleClicked = button;
                  blockUI = false;
                  commandType = 'ButtonItem';
                  eventArgs.set_cancel(true)
              }
          }
      }

      //**********************************************************
      // GESTIONE SELEZIONE GRIGLIA
      //**********************************************************
      function RowSelecting(sender, eventArgs) {
          var stato = document.getElementById('StatoPaginaHidden').value;
          if (stato == 'C') {
              var grid = sender;
              masterTable = grid.get_masterTableView();
              blockUI = false;
              showHideConfirm();
              commandName = eventArgs.get_commandName();
              commandArgument = eventArgs.get_commandArgument();
              commandType = 'GridItem';
              eventArgs.set_cancel(true);
          }
      }

      //**********************************************************
      // GESTIONE TOOLBAR MODULI 
      //**********************************************************
      function OnToolBarModuliClick(sender, eventArgs) {
          var stato = document.getElementById('StatoPaginaHidden').value;
          if (stato == 'C') {
              blockUI = true;
              showHideConfirm();
              commandType = 'ToolbarItem';
              eleClicked = sender;
              eventArgs.set_cancel(true);
          }
      }

      //**********************************************************
      // GESTIONE CLICK DA CODICE
      //**********************************************************
      function FireCommand() {
          if (commandType == 'GridItem') { masterTable.fireCommand(commandName, commandArgument); }
          if (commandType == 'ButtonItem') { eleClicked.click(); }
          if (commandType == 'MenuItem') { eleClicked.click(); }
          if (commandType == 'ToolbarItem') { eleClicked.click(); }
      }




      var element;
      var validNavigation = false;
      
      
      document.onkeydown = function (e) {
          try {
              if (!e) var e = window.event;
              var keyCode = e.keyCode ? e.keyCode : e.which ? e.which : e.charCode;
              // var event = jQuery.event.fix(e || window.event);
              // var keycode = event.which;
              if ((keyCode == 116) || (e.ctrlKey && keyCode == 82)) {
                  validNavigation = true;
              }
              //var isControl = event.ctrlKey;
          }
          catch (e) { }
      }



       

        document.onmousemove = function (e) {
            try {
                e = e || window.event;

               
                try {
                    if ((element.href === "javascript:void(0)") || (element.href === undefined)) {
                        validNavigation = true;
                    } else {
                        element = null;
                        validNavigation = false;
                    }
                }
                catch (e) { }
                document.getElementById('Debug').value = element.href;
            }
            catch (e) { }
        }

        document.onclick = function (e) {
            try {
                e = e || window.event;
                element = e.target || e.srcElement;
                validNavigation = true;
                document.getElementById('Debug').value = 'Click ' + validNavigation;
            }
            catch (e) { }
        }


      document.onmousedown = function (e) {
          try {
              e = e || window.event;
              element = e.target || e.srcElement;
              validNavigation = true;
              document.getElementById('Debug').value = 'MouseDown ' + validNavigation;
          }
          catch (e) { }
      }


      window.onbeforeunload = function (e) {
          try {
              CloseApplication(e);
          }
          catch (e) {
          }
      }


      

      function WireUpEvents() {

          //ASSOCIO L'EVENTO CLICK A TUTTI I LINK DELLA PAGINA
          var links = document.getElementsByTagName('a');
          for (i = 0; i < links.length; i++) {
              addEvent(links[i], 'click', function () { validNavigation = true; });
          }

          document.forms[0].onSubmit = function () { validNavigation = true; };

          document.onclick = function () { validNavigation = true; };

          var inputs = document.getElementsByTagName('input');
          for (i = 0; i < inputs.length; i++) {

              var submit = (/submit/).test(inputs[i].getAttribute("type"));
              if (submit == true) {
                  inputs[i].onclick = function () { validNavigation = true; };
              }

              var image = (/image/).test(inputs[i].getAttribute("type"));
              if (image == true) {
                  inputs[i].onclick = function () { validNavigation = true; };
              }



              //var pagerInput = (/rcbInput/).test(inputs[i].className);
              //if (pagerInput == true) {
                  //inputs[i].onclick = function () { validNavigation = true; };
              //}

          }


      }




      window.onload = function () {





          //WireUpEvents();



          //Nascondo la notifica
          document.getElementById('notification').style.display = 'none';

          document.getElementById('notification').style.bottom = '0px';
          document.getElementById('notification').style.right = '0px';
          document.getElementById('notification').style.position = 'absolute';
          document.getElementById('notification').style.zIndex = 50000000000;

          document.getElementById('notificationClose').onclick = function () { document.getElementById('notification').style.display = 'none'; };


          //QUESTA FUNZIONE RIATTIVA IL TIMEOUT 



          GetServerData();

          CheckNewTips();




          //Carico le impostazioni.
          loadSettings();

          if (isOpen == "false") {
              //Lo nascondo.

              document.getElementById('leftPanel').style.display = 'none';
              document.getElementById('imgMenu').className = 'MenuOn';
              document.getElementById('imgMenu').alt = 'Visualizza Menu';

          } else {
              //Lo visualizzo.

              document.getElementById('leftPanel').style.display = '';
              document.getElementById('leftPanel').style.width = '220px';
              document.getElementById('imgMenu').className = 'MenuOff';
              document.getElementById('imgMenu').alt = 'Nascondi Menu';

          }

          if (document.getElementById('separator').addEventListener) {
              document.getElementById('separator').addEventListener('click', MenuOnOff, false);
          } else if (document.getElementById('separator').attachEvent) {
              document.getElementById('separator').attachEvent('onclick', MenuOnOff);
          }




      }



      function MenuOnOff() {
         
          if (isOpen == "true") {
              document.getElementById('leftPanel').style.display = 'none';
              document.getElementById('imgMenu').className = 'MenuOn';
              document.getElementById('imgMenu').alt = 'Visualizza Menu';
              isOpen = "false";


          } else {

              document.getElementById('leftPanel').style.display = '';
              document.getElementById('leftPanel').style.width = '220px';
              document.getElementById('imgMenu').className = 'MenuOff';
              document.getElementById('imgMenu').alt = 'Nascondi Menu';
              isOpen = "true";
           }
          //Salvo le impostazioni.
          saveSettings('Menu', isOpen);
      }

      function loadSettings() {
          var start = document.cookie.indexOf(COOKIE_NAME + "=");
          if (start == -1) return;
          start += COOKIE_NAME.length + 1;
          var end = document.cookie.indexOf(";", start);
          if (end == -1) end = document.cookie.length;
          var cookieValue = unescape(document.cookie.substring(start, end));
          var pair = cookieValue.split(":");
          isOpen = pair;
      }


       function saveSettings(key, value) {
          var today = new Date();
          var expirationDate = new Date(today.getTime() + 365 * 1000 * 60 * 60 * 24);
          document.cookie = COOKIE_NAME + "=" + value + ";expires=" + expirationDate.toGMTString();
      }




    
 

    </script>

