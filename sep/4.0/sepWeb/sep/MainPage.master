            function CreateXMLHttpRequest() {
                if (window.ActiveXObject) {
                    AJAX = new ActiveXObject("Microsoft.XMLHTTP");
                }
                else {
                    AJAX = new XMLHttpRequest();
                }
            }



            function CreateXMLHttpRequest2() {
                if (window.ActiveXObject) {
                    AJAX2 = new ActiveXObject("Microsoft.XMLHTTP");
                }
                else {
                    AJAX2 = new XMLHttpRequest();
                }
            }



            var attivaNotificaSegnalazioni = '<%= GetSegnalazioni %>';


            function CheckNewTips() {
                if (attivaNotificaSegnalazioni == '1') {
                    CreateXMLHttpRequest2();
                    if (AJAX2 == null) {
                        alert('Il browser non supporta Ajax');
                    }
                    else {
                        AJAX2.open("GET", '<%=ResolveUrl("~/NotificaSegnalazioniPage.aspx")%>?check=' + Math.random(), true);
                        AJAX2.send(null);
                    }
                }
            }


          
          
          var attivaNotifica = '<%=AttivaNotifica %>';


          function GetServerData() {
              if (attivaNotifica == 'true') {
                  CreateXMLHttpRequest();
                  if (AJAX == null) {
                      alert('Il browser non supporta Ajax');
                  }
                  else {
                      if (attivaNotifica == 'true') {
                          AJAX.open("GET", '<%=ResolveUrl("~/NotificaPage.aspx")%>?checkDocumenti=' + Math.random(), true);
                      }
                      AJAX.onreadystatechange = ClientSideUpdate;
                      AJAX.send(null);
                  }
                  window.setTimeout(function () { GetServerData(); }, 10000);
              }
          }

            function ClientSideUpdate() {
                var result;
              
                if (AJAX.readyState == 4) {
                   
                    result = AJAX.responseText;
                  
                    if (result != '') {
                      
                        var n = result.indexOf("<div>");
                      
                        if (n == 0) {
                             document.getElementById('notificationIn').innerHTML = result;
                            var obj = document.getElementById('notification');
                            slideDown(obj);
                       }
                      //try  { } catch (e) { }
                        

                    }


                }
            }



           function slideDown(elem) {

                var h = 175;
                elem.style.height = "0px";
                elem.style.display = "block";
                elem.style.overflow = "hidden";
               

                var t = "";
                for (var i = 0; i <= 100; i += 5) {

                    (function () {
                        var pos = i;

                        t = setTimeout(function () {
                            elem.style.height = (pos / 100) * h + "px";

                        }, (pos + 1) * 10);
                    })();

                }

            }

        

  </script>


  

